{
    "install": {
        "codeigniter4": [
            "app/Config/Routes/Article.php",
            "app/Controllers/Article.php",
            "app/Database/Migrations/2022-09-19-071033_Article.php",
            "app/Database/Seeds/Article.php",
            "app/Models/ModArticle.php"
        ],
        "panel": [
            "vue3admin/src/api/article.js",
            "vue3admin/src/views/article/index.vue",
            "vue3admin/src/views/article/edit.vue",
            "vue3admin/src/views/article/add.vue"
        ],
        "menu": {
            "path": "/articleList",
            "children": [
              {
                "path": "index",
                "component_import": "../views/article/index.vue",
                "name": "articleList",
                "meta": {
                  "title": "文章管理",
                  "icon": "user"
                }
              },
              {
                "path": "add",
                "component_import": "../views/article/add.vue",
                "name": "articleAdd",
                "hidden": true,
                "meta": {
                  "title": "添加文章",
                  "icon": "user"
                }
              },
              {
                "path": "edit/:id",
                "component_import": "../views/article/edit.vue",
                "name": "articleEdit",
                "hidden": true,
                "meta": {
                  "title": "编辑文章",
                  "icon": "user"
                }
              }
            ]
          },
        "shell": [
            "php spark migrate -all",
            "apidoc -i app/Controllers/ -o public/doc",
            "cd vue3admin",
            "pnpm install @jsdawn/vue3-tinymce"
        ]
    },
    "package": {
        "codeigniter4": [],
        "panel": []
    }
}